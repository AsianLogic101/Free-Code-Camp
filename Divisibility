function divisible(number,x,y) {;
    if (number % x === 0 && number % y === 0) {
        document.write(number," is divisible by ",x," and ",y);
        document.write("<br>");
    } else if (number % x === 0) {
        document.write(number," Divisble by ",x," only");
        document.write("<br>");
    } else if (number % y == 0) {
        document.write(number," Divible by ",y," only");
        document.write("<br>");
    } else {
        document.write(number, " Not divisable by ",x,"or",y);
    }
}
divisible(10,4,32);
divisible(9,1,1);
divisible(3,4,0);
